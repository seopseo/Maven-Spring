<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="co.seop.pjt.member.service.MemberService">
  	<select id="memberSelectList" resultType="co.seop.pjt.member.service.MemberVO">
  		select * from member	
  	</select>
  	
  	<select id="memberSelect" 
	  	parameterType="co.seop.pjt.member.service.MemberVO"
	  	resultType="co.seop.pjt.member.service.MemberVO">
  		select * from member where id = #{id}
  	</select>
  
  	<insert id="memberInsert"></insert>
  	<update id="memberUpdate"></update>
  	<delete id="memberDelete"></delete>
  </mapper>